{
  "Arrow function": {
    "prefix": ["fn"],
    "body": [
      "fn ($1) => $0"
    ],
    "description": "Arrow function"
  },
  "Public method": {
    "prefix": ["pub"],
    "body": [
    "public function ${1:name}(${2:arg}): ${3:void}",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "Public method"
  },
  "Private method": {
    "prefix": ["pri"],
    "body": [
    "private function ${1:name}(${2:arg}): ${3:void}",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "Private method"
  },
  "Constructor": {
    "prefix": ["con"],
    "body": [
    "public function __construct(${1:arg})",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "Constructor"
  },
  "Invoke method": {
    "prefix": ["inv"],
    "body": [
    "public function __invoke(${1:arg}): ${2:void}",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "__invoke method"
  },
  "Make method": {
    "prefix": ["mak"],
    "body": [
    "public static function make(${1:arg}): ${2:void}",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "Make static method"
  },
  "Handle method": {
    "prefix": ["han"],
    "body": [
    "public static function handle(${1:arg}): ${2:void}",
    "{",
    "\t${0:target}",
    "}"
    ],
    "description": "Handle static method"
  },
  "Try catch": {
    "prefix": ["try"],
    "body": [
      "try {",
      "} catch (${2:exception}) {",
      "\t${0:target}",
      "}"
    ],
    "description": "Handle static method"
  }
}
